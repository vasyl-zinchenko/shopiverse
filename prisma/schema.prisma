generator client {
  provider = "prisma-client-js"
}
 
datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}
 
model Products {
  id          Int       @id @default(autoincrement())
  title       String
  description String
  image       String
  category    String
  price       Decimal
	rating_rate 			Decimal
	rating_count 			Decimal
  created_at  DateTime? @default(now()) @db.Timestamptz(6)
}